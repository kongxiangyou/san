package org.db.psd.dao;







public interface SplitDAO {

	/**
	 * 总页数 
	 * @param rows 总行数 
	 * @param size 每页的显示数量 
	 * @return
	 */
	public long allPages(long rows , int size);
	
	/**
	 * 数据库中查询数据的开始位置 
	 * @param nowpage 当前访问的页数
	 * @param size 每页的显示数量 
	 * @return
	 */
	public long currentRows(int nowpage , int size);
	
	
	/**
	 * 总行数
	 * @return
	 */
	public long allRows() ;
	
}

